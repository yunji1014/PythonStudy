n, m = map(int, input().split())

r = 0

for i in range(n):
    data = list(map(int, input().split()))

    minV = 10001
    for a in data:
        minV = min(minV, a)

    r = max(r, minV)

print(r)

-------------------------------------
n, m = map(int, input().split())

r = 0

for i in range(n):
    data = list(map(int, input().split()))
    min_value = min(data)

    r = max(r, min_value)

print(r)
